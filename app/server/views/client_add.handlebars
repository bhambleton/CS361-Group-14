<div id="addContainer">
    <div id="resultsContent">
        <h3><p><span id="providerHeader3">Add Service</span></p></h3>
        <form action="/client/result_add_update_delete_claim" method="POST">
            <input type="hidden" name="action" value="add"/>
            <label>Name: <input type="text" name="name" placeholder="Service Name" required>  </label> <br/>
            <label>Type: <input type="text" name="type" placeholder="Service Type" required>  </label> <br/>
            <label>Address: <input type="text" name="address" placeholder="Service Street Address" required>  </label> <br/>
            <label>City: <input type="text" name="city" placeholder="Service City" required>  </label> <br/>
            <label>State: <input type="text" name="state" placeholder="Service State" required>  </label> <br/>
            <label>Zip: <input type="text" name="zip" placeholder="Service Zip" required>  </label> <br/>
            <label>Phone: <input type="tel" name="phone" placeholder="Service Phone">  </label> <br/>
            <label>Email: <input type="email" name="email" placeholder="Service Email" >  </label>	 <br/>
            <label>Notes: <input type="text" name="notes" placeholder="Service Notes/Info" >  </label>	 <br/>

            <div class="navItemContainer">
                <input type="submit" class="navButton" id="regSubmitButton">
                    <i class="providerIcon fas fa-hand-point-right" id="regSubmitIcon" title="Submit Registration"></i>
                </input>
            </div>
        </form>

    </div>
</div>